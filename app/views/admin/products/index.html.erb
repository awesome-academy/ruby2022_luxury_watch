<div class="col-sm-10 main">
  <h1 class="page-header"><%= t ".title" %></h1>
  <%== pagy_bootstrap_nav(@pagy) %>
  <%= link_to t(".create"), new_admin_product_path, class: "btn btn-primary" %>
  <div class="table-responsive">
    <table class="table table-striped">
      <thead>
        <tr>
          <th><%= t "name" %></th>
          <th><%= t "images" %>
          <th><%= t "quantity" %></th>
        </tr>
      </thead>
      <tbody>
        <%= render @products %>
      </tbody>
    </table>
  </div>
</div>
